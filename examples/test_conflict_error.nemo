# Test that conflict detection works for directly-defined conflicting methods

Parent1 := Object derive: #(a)
Parent1 >> foo [ ^ "foo1" ]

Parent2 := Object derive: #(b)
Parent2 >> bar [ ^ "bar2" ]

# Create a child without overriding foo
ChildNoOverride := Object derive: #(x)
ChildNoOverride >> baz [ ^ "baz" ]

# Try to add Parent1 (should work, no conflict)
ChildNoOverride addParent: Parent1

# Now try to add Parent2 that has a different directly-defined method "bar"
# This should also work since there's no conflict
ChildNoOverride addParent: Parent2

result := "OK"
result
